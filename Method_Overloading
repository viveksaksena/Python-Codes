class abc:
    def __init__(self):
        pass

    def b(self, *args):
       y=0
       for x in args:
           y=y+x
       return y

    def area(self,r):
        pi=3.14
        return r*r*pi


class b(abc):
    def area(self,r):
        pi=3.14
        return r*r*pi

obj= abc()
bb= b()
print(bb.area(4))
print(obj.b(23,87,23,43,21))
